<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
	<rule>
		<note>登录重写地址，其中form为访问地址,to为实际隐藏地址</note><!--规则描述 -->
		<from>/zhouyajuan</from><!--显示的跳转路径（同时也是href,action里跳转的路径），相当于是显示的具体路径 -->
		<to>/main/main.jsp</to><!--实际页面路径 -->
	</rule>
	<rule>
		<from>http://localhost:8080/sms-wsp/main/main.jsp</from>
		<to>/../sms-wsp/main/dafaf.jsp</to>
	</rule>
	<outbound-rule>
		<from>http://localhost:8080/sms-wsp/main/main.jsp</from>
		<to>/sms-wsp/main/fafafafa.html</to>
	</outbound-rule>
</urlrewrite>
